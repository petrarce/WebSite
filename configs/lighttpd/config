server.document-root = env.DOCUMENT_ROOT + "/www.example.org/pages/" 

server.port = 3000

server.username = "www"
server.groupname = "www"

mimetype.assign = (
  ".html" => "text/html", 
  ".txt" => "text/plain",
  ".jpg" => "image/jpeg",
  ".png" => "image/png" 
)

static-file.exclude-extensions = ( ".fcgi", ".php", ".rb", "~", ".inc" )
index-file.names = ( "index.html" )